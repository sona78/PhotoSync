{
  "name": "photosync",
  "version": "1.0.0",
  "description": "An Electron application",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run electron:external\"",
    "start:webrtc": "node webrtc-server.js",
    "electron": "electron .",
    "electron:external": "cross-env EXTERNAL_SERVER=true electron .",
    "dev": "electron .",
    "build": "electron-builder",
    "server": "node server.js",
    "generate-cert": "node generate-cert.js",
    "generate-ca": "node generate-ca.js && node generate-ios-profile.js",
    "setup-certificates": "npm run generate-ca && npm start"
  },
  "keywords": [
    "electron"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "@roamhq/wrtc": "^0.8.0",
    "@supabase/supabase-js": "^2.91.0",
    "dotenv": "^17.2.3",
    "electron-store": "^11.0.2",
    "express": "^5.2.1",
    "node-forge": "^1.3.3",
    "openssl": "^2.0.0",
    "qrcode": "^1.5.4",
    "sharp": "^0.34.5",
    "simple-peer": "^9.11.1",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5"
  }
}
